CREATE TABLE PERSON (TITLE VARCHAR(255) NOT NULL, FIRSTCONTACT BIGINT, FRIENDSSINCE BIGINT, HASPROPERTIES TINYINT(1) default 0, IS_STILL_PARTICIPANT TINYINT(1) default 0, LASTCONTACT BIGINT, LISTENERS LONGBLOB, MESSAGES LONGBLOB, NACHFASSEN BIGINT, NOTES VARCHAR(255), STATE VARCHAR(255), THREAD_PATH VARCHAR(255), THREAD_TYPE VARCHAR(255), PRIMARY KEY (TITLE))
